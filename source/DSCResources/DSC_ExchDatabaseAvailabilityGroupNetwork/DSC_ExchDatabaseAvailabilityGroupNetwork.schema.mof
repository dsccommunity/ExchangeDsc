
[ClassVersion("1.0.0.0"), FriendlyName("ExchDatabaseAvailabilityGroupNetwork")]
class DSC_ExchDatabaseAvailabilityGroupNetwork : OMI_BaseResource
{
    [Key, Description("The name of the DAG network.")] String Name;
